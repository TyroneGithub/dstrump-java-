<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="450.0" prefWidth="500.0">
         <children>
            <Label alignment="CENTER" prefHeight="146.0" prefWidth="500.0" text="TIC TAC TOE">
               <font>
                  <Font name="Roboto Light" size="33.0" />
               </font>
            </Label>
            <Button fx:id="playButt" onAction = "#setPlayStage" alignment="CENTER" mnemonicParsing="false" prefHeight="42.0" prefWidth="172.0" text="PLAY">
               <font>
                  <Font name="Roboto Light" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets left="160.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="exitButt" onAction = "#closeStage" alignment="CENTER" layoutX="170.0" layoutY="156.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="172.0" text="EXIT">
               <font>
                  <Font name="Roboto Light" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets left="160.0" top="20.0" />
               </VBox.margin>
            </Button>
         </children></VBox>
   </children>
</GridPane>
